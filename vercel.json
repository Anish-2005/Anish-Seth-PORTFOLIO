{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "anishseth.vercel.app"
        }
      ],
      "destination": "https://anishseth.xyz/:path*",
      "permanent": true
    }
  ]
}
